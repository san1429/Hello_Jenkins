    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "app 2 is building" 
            }
        }
        stage('Test') { 
            steps {
                echo "app 2 is under testing"
            }
        }
        stage('Deploy') { 
            steps {
                echo "app 2 is deployed"
            }
        }
    }
}
